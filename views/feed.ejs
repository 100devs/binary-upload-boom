<%- include('partials/navbarheader') -%>
  <div class="container">
    <div class="row justify-content-center mt-5">
      <ul class="row list-unstyled">

        <p>Your photos have gotten a total of <b><%= likes[0].total %></b> likes!</p>


        <% for(var i=0; i<posts.length; i++) {%>
          <li class="col-6 justify-content-between mt-5">
            <a href="/post/<%= posts[i]._id%>">
              <img class="img-fluid" src="<%= posts[i].image%>">
            </a>
            <p><%= posts[i].title %></p>
            <p>"<%= posts[i].caption %>"</p>
            <p>Likes: <%= posts[i].likes %></p>
            <p>Published: <%= posts[i].createdAt %></p>


            <p>Published by: <a href="/publicProfile/<%=posts[i].user._id%>"><%= posts[i].user.userName%></a></p>
          </li>
        <% } %>
        </ul>
    </div>
  </div>  
<%- include('partials/footer') -%>